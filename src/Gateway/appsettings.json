{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=/data/Gateway.db"
  },
  "JwtOptions": {
    "Secret": "Das*ist*ein*tokasdf*Passwort*123@#$%^",
    "ExpireDay": 30
  },
  "RequestOptions": {
    "FilterSuffixes": [
      ".css",
      ".jpg",
      ".png",
      ".gif",
      ".ico",
      ".svg",
      ".woff",
      ".woff2",
      ".ttf",
      ".js.map",
      ".css.map"
    ],
    "FilterContentTypes": [
      "text/css",
      "image/jpeg",
      "image/png",
      "image/gif",
      "image/x-icon",
      "image/svg+xml",
      "font/woff",
      "font/woff2",
      "font/ttf"
    ]
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "cluster1/destination1": {
            "Address": "http://backend1.app/"
          }
        }
      }
    }
  }
}